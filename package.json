{
  "name": "project-management-demo-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "typesync || :",
    "setup": "husky install",
    "dev": "next dev -p 4001",
    "build": "next build",
    "start": "next start -p 8080",
    "start:local": "yarn build && yarn start",
    "commit": "yarn precommit && git-cz",
    "precommit": "tsc && lint-staged",
    "lint": "eslint --ext .js,.ts,.tsx 'src/**'",
    "lint:fix": "eslint --ext .js,.ts,.tsx 'src/**' --fix",
    "test": "jest",
    "test:ci": "jest --maxWorkers=1",
    "tsc": "tsc",
    "tsc:scripts": "tsc --p ./scripts/",
    "ts-node": "ts-node",
    "codegen": "yarn codegen:download && graphql-codegen",
    "codegen:watch": "yarn codegen:download && graphql-codegen --watch",
    "codegen:download": "env-cmd -f ./.env.local node scripts/codegen/download.js",
    "codegen:prod": "yarn codegen:download:prod && graphql-codegen",
    "codegen:download:prod": "echo ENV_JSON > ./env.json && env-cmd -f ./.env.json node scripts/codegen/download.js",
    "codegen:ci": "graphql-codegen",
    "new:queryMock": "ts-node ./scripts/newQueryMock.ts",
    "new:mutationMock": "ts-node ./scripts/newMutationMock.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic",
    "chromatic:local": "env-cmd -f ./.env.local npx chromatic"
  },
